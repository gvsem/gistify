{
	"name": "gistify",
	"displayName": "Gistify",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Gistify",
			"properties": {
				"gistify.pastebin.apiToken": {
					"type": "string",
					"default": "",
					"description": "Specify Pastebin developer token for this extension to act from."
				},
				"gistify.pastebin.defaultUserName": {
					"type": "string",
					"default": "",
					"description": "Default user name to login in Pastebin."
				},
				"gistify.pastebin.userToken": {
					"type": "string",
					"default": "",
					"description": "Specify user token in Pastebin to upload snippets."
				},
				"gistify.gists.apiToken": {
					"type": "string",
					"default": "",
					"description": "Specify Github Gists api token for this extension to act from."
				},
				"gistify.gists.userToken": {
					"type": "string",
					"default": "",
					"description": "Specify Github token to use Gists."
				},
				"gistify.storage": {
					"type": "string",
					"default": "{}",
					"description": "Workspace extension settings, do not edit."
				}
			}
		},
		"commands": [
			{
				"command": "gistify.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"github-api": "^3.4.0",
		"request": "^2.88.2"
	}
}
